<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chaoxing.activity.mapper.SignUpConditionMapper">
    <resultMap id="BaseResultMap" type="com.chaoxing.activity.model.SignUpCondition">
        <id column="id" jdbcType="INTEGER" property="id" />
        <result column="template_id" jdbcType="INTEGER" property="templateId" />
        <result column="component_id" jdbcType="INTEGER" property="componentId" />
        <result column="origin_identify" jdbcType="VARCHAR" property="originIdentify" />
        <result column="field_name" jdbcType="VARCHAR" property="fieldName" />
        <result column="is_allow_signed_up" jdbcType="BIT" property="allowSignedUp" />
    </resultMap>
    <sql id="Base_Column_List">
        t.id, t.template_id, t.component_id, t.origin_identify, t.field_name, t.is_allow_signed_up
    </sql>

    <insert id="batchAdd">
        INSERT INTO t_sign_up_condition ( template_id, component_id, origin_identify, field_name, is_allow_signed_up )
        VALUES
            <foreach collection="signUpConditions" index="key" item="value" open="" close="" separator=",">
                ( #{value.templateId}, #{value.componentId}, #{value.originIdentify}, #{value.fieldName}, #{value.allowSignedUp} )
            </foreach>
    </insert>
</mapper>