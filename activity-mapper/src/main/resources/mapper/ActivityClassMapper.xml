<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chaoxing.activity.mapper.ActivityClassMapper">
    <resultMap id="BaseResultMap" type="com.chaoxing.activity.model.ActivityClass">
        <result column="activity_id" jdbcType="INTEGER" property="activityId" />
        <result column="class_id" jdbcType="INTEGER" property="classId" />
        <result column="create_time" jdbcType="TIMESTAMP" property="createTime" />
        <result column="update_time" jdbcType="TIMESTAMP" property="updateTime" />
    </resultMap>

    <insert id="batchAdd">
        INSERT INTO t_activity_class ( activity_id, class_id )
        VALUES
        <foreach collection="dataList" index="key" item="value" open="" close="" separator=",">
            ( #{value.activityId}, #{value.classId} )
        </foreach>
    </insert>
</mapper>