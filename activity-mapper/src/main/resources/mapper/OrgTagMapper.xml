<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chaoxing.activity.mapper.OrgTagMapper">
    <resultMap id="BaseResultMap" type="com.chaoxing.activity.model.OrgTag">
        <id column="id" jdbcType="INTEGER" property="id" />
        <result column="fid" jdbcType="INTEGER" property="fid" />
        <result column="tag_id" jdbcType="INTEGER" property="tagId" />
        <result column="sequence" jdbcType="INTEGER" property="sequence" />
        <result column="create_time" jdbcType="TIMESTAMP" property="createTime" />
        <result column="update_time" jdbcType="TIMESTAMP" property="updateTime" />
    </resultMap>
    <sql id="Base_Column_List">
        t.id, t.fid, t.tag_id, t.sequence, t.create_time, t.update_time
    </sql>

    <insert id="batchAdd">
        INSERT INTO t_org_tag( fid, tag_id )
        VALUES
        <foreach collection="tagIds" index="key" item="value" open="" close="" separator=",">
            ( #{fid}, #{value} )
        </foreach>
    </insert>

</mapper>